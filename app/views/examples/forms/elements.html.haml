			.body
				%form{:role => "form"}
					%fieldset
						%legend.section Default Form
						.row
							.col-sm-8
								.form-group
									%label{:for => "search-input"} Search type input
									.input-group
										%input#search-input.form-control{:type => "search"}
											%span.input-group-btn
												%button.btn.btn-inverse{:type => "button"} Search
								.form-group
									%label{:for => "bar"} Whole bar appended
									.input-group
										%input#bar.form-control{:type => "text"}
											.input-group-btn
												%button.btn.btn-danger{:type => "button"}
													%i.fa.fa-pencil
												%button.btn.btn-warning{:type => "button"}
													%i.fa.fa-plus
												%button.btn.btn-success{:type => "button"}
													%i.fa.fa-refresh
								.form-group
									%label{:for => "dropdown-appended"} Actions dropdown
									.input-group
										%input#dropdown-appended.form-control{:type => "text"}
											.input-group-btn
												%button.btn.btn-success.dropdown-toggle{"data-toggle" => "dropdown"}
													Action
													%i.fa.fa-caret-down
												%ul.dropdown-menu
													%li
														%a{:href => "#"} Action
													%li
														%a{:href => "#"} Another action
													%li
														%a{:href => "#"} Something else here
													%li.divider
													%li
														%a{:href => "#"} Separated link
								.form-group
									%label{:for => "type-dropdown-appended"} Types dropdown
									.input-group
										%input#type-dropdown-appended.form-control{:type => "text"}
											.input-group-btn
												%select#phone-type.selectpicker{"data-style" => "btn-primary", "data-width" => "auto"}
													%option Another type
													%option Type one
													%option Next type
									%p.help-block You can select some type of a field just right in the place.
								.form-group
									%label{:for => "segmented-dropdown"} Segmented dropdown
									.input-group
										%input#segmented-dropdown.form-control{:type => "text"}
											.input-group-btn
												%button.btn.btn-warning{:tabindex => "-1"} Action
												%button.btn.btn-warning.dropdown-toggle{"data-toggle" => "dropdown"}
													%i.fa.fa-caret-down
												%ul.dropdown-menu
													%li
														%a{:href => "#"} Action
													%li
														%a{:href => "#"} Another action
													%li
														%a{:href => "#"} Something else here
													%li.divider
													%li
														%a{:href => "#"} Separated link
									%span.help-block Anything can be appended to the right
					.form-actions
						.btn-toolbar
							%button.btn.btn-inverse{:type => "submit"} Save Changes
							%button.btn.btn-default{:type => "button"} Cancel
.row
