.row
	.col-md-12
		%h2.page-title
			Form Elements
			%small Inputs, controls and more
.row
	.col-md-6
		%section.widget
			%header
				%h4
					%i.fa.fa-align-left
					Inputs
				.widget-controls
					%a{:href => "#"}
						%i.glyphicon.glyphicon-cog
					%a{:href => "#"}
						%i.fa.fa-refresh
					%a{"data-widgster" => "close", :href => "#"}
						%i.glyphicon.glyphicon-remove
			.body
				%form.form-horizontal{:role => "form"}
					%fieldset
						%legend.section Horizontal form
						.form-group
							%label.col-sm-4.control-label{:for => "normal-field"} Normal field
							.col-sm-7
								%input#normal-field.form-control{:placeholder => "May have placeholder", :type => "text"}
						.form-group
							%label.col-sm-4.control-label{:for => "hint-field"}
								Transparent input
							.col-sm-7
								%input#transparent-field.form-control.input-transparent{:placeholder => "Stylish, huh??", :type => "text"}
						.form-group
							%label.col-sm-4.control-label{:for => "hint-field"}
								Label hint
								%span.help-block Some help text
							.col-sm-7
								%input#hint-field.form-control.input-transparent{:type => "text"}
						.form-group
							%label.col-sm-4.control-label{:for => "tooltip-enabled"} Tooltip enabled
							.col-sm-7
								%input#tooltip-enabled.form-control.input-transparent{"data-original-title" => "Some explanation text here", "data-placement" => "top", :placeholder => "Hover over me..", :type => "text"}
						.form-group
							%label.col-sm-4.control-label{:for => "disabled-input"} Disabled input
							.col-sm-7
								%input#disabled-input.form-control.input-transparent{:disabled => "disabled", :type => "text", :value => "Default value"}
						.form-group
							%label.col-sm-4.control-label{:for => "max-length"} Max length
							.col-sm-7
								%input#max-length.form-control.input-transparent{"data-original-title" => "You cannot write more than 3 characters.", "data-placement" => "top", :maxlength => "3", :placeholder => "Max length 3 characters", :type => "text"}
						.form-group
							%label.col-sm-4.control-label{:for => "prepended-input"} Prepended input
							.col-sm-7
								.input-group
									%span.input-group-addon
										%i.fa.fa-user
									%input#prepended-input.form-control.input-transparent{:placeholder => "Username", :size => "16", :type => "text"}
						.form-group
							%label.col-sm-4.control-label{:for => "password-field"} Password
							.col-sm-7
								.input-group
									%span.input-group-addon
										%i.fa.fa-lock
									%input#password-field.form-control.input-transparent{:placeholder => "Password", :type => "password"}
						.form-group
							%label.col-sm-4.control-label{:for => "appended-input"} Appended input
							.col-sm-7
								.input-group
									%input#appended-input.form-control.input-transparent{:size => "16", :type => "text"}
										%span.input-group-addon .00
						.form-group
							%label.col-sm-4.control-label{:for => "combined-input"} Combined
							.col-sm-7
								.input-group
									%span.input-group-addon $
									%input#combined-input.form-control.input-transparent{:size => "16", :type => "text"}
										%span.input-group-addon .00
						.form-group
							%label.col-sm-4.control-label{:for => "transparent-input"}
								Append Transparent
							.col-sm-7
								.input-group
									%input#transparent-input.form-control.input-transparent{:type => "text"}
										%span.input-group-addon
											%i.fa.fa-camera
					.form-actions
						.row
							.col-sm-offset-4.col-sm-7
								.btn-toolbar
									%button.btn.btn-primary{:type => "submit"} Save Changes
									%button.btn.btn-inverse{:type => "button"} Cancel
	.col-md-6
		%section.widget
